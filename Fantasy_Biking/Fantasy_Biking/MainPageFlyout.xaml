<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Fantasy_Biking.MainPageFlyout"
             Title="Flyout">
    <ContentPage.Content>
        <StackLayout>
            <Label
                Text="Navigation"
                VerticalOptions="Center" HorizontalOptions="Center" Padding="0,30,0,30" FontSize="Title"/>
            <Button
                 x:Name="Races" Text="Races" Clicked="Races_Clicked"/>
            <Button
                 x:Name="Cyclists" Text="Cyclists" Clicked="Cyclists_Clicked"/>
            <Button
                x:Name="Leaderboard" Text="Leaderboard" Clicked="Leaderboard_Clicked"/>
            <Button
                x:Name="Notes" Text="Notes" Clicked="Notes_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>